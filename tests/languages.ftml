  <testgroup label="#40 language support - Khamti Shan">
    <test label="default">
      <comment>open dots</comment>
      <string>\u1036 \u1038 \u1087 \u1088 \u1089 \u108A \u109A \u109B \uAA7B \u1086</string>
    </test>
    <test label="khn - Khandeshi" stylename="khn">
      <comment>filled dots</comment>
      <string>\u1036 \u1038 \u1087 \u1088 \u1089 \u108A \u109A \u109B \uAA7B \u1086</string>
    </test>
    <test label="kht - Khamti" stylename="kht">
      <comment>filled dots</comment>
      <string>\u1036 \u1038 \u1087 \u1088 \u1089 \u108A \u109A \u109B \uAA7B \u1086</string>
    </test>
    <test label="cv01" stylename="cv01_1">
      <comment>filled dots except last</comment>
      <string>\u1036 \u1038 \u1087 \u1088 \u1089 \u108A \u109A \u109B \uAA7B \u1086</string>
    </test>
    <test label="ss01" stylename="ss01_1">
      <comment>filled dots except last</comment>
      <string>\u1036 \u1038 \u1087 \u1088 \u1089 \u108A \u109A \u109B \uAA7B \u1086</string>
    </test>
    <test label="cv06" stylename="cv06_1">
      <comment>filled dots only last</comment>
      <string>\u1036 \u1038 \u1087 \u1088 \u1089 \u108A \u109A \u109B \uAA7B \u1086</string>
    </test>
    <test label="ss06" stylename="ss06_1">
      <comment>filled dots only last</comment>
      <string>\u1036 \u1038 \u1087 \u1088 \u1089 \u108A \u109A \u109B \uAA7B \u1086</string>
    </test>
  </testgroup>
